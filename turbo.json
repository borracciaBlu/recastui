{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"],
			"cache": false
		},
		"test": {
			"outputs": [],
			"inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx"]
		},
		"typecheck": {
			"cache": false
		},
		"dev": {
			"cache": false
		},
		"clean": {
			"cache": false
		}
	}
}
